{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "lib": [
      "es2015",
    ],
    "jsx": "react",
    "sourceMap": false,
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "types": [
      "react",
      "react-native",
      "jest"
    ],
    "removeComments": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "noImplicitThis": false,
    "alwaysStrict": false,
    "allowSyntheticDefaultImports": true,
  }
}